{
  "manifest_version": 3,
  "name": "OpenClaw Remote",
  "version": "1.0.0",
  "description": "OpenClaw AI远程控制浏览器",
  "permissions": [
    "activeTab",
    "scripting",
    "tabs",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
